select * FROM
store.customers a JOIN
store.orders b
on a.customerId=b.customerId
WHERE
email='jjj@y.com'

select * from 
store.orders a JOIN
store.order_items b
on a.orderId=b.orderId JOIN 
store.products c
on b.productId=c.productId
where a.orderId=1


select * FROM
customers a JOIN
orders b
on a.customerId=b.customerId
WHERE
a.email='jjj@y.com'


select * from 
orders a JOIN
order_items b
on a.orderId=b.orderId JOIN 
products c
on b.productId=c.productId
where a.orderId=1